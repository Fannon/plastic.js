{
	"tags": {
		"allowUnknownTags" : true
	},
	"plugins": ["plugins/markdown"],
	"templates": {
		"cleverLinks": false,
		"monospaceLinks": false,
		"default": {
			"outputSourceFiles" : false
		},

		"applicationName": "plastic.js Dev Docs",
		"disqus": "",
		"googleAnalytics": "",
		"openGraph": {
			"title": "",
			"type": "website",
			"image": "",
			"site_name": "",
			"url": ""
		},
		"meta": {
			"title": "plastic.js API Documentation",
			"description": "plastic.js API Documentation",
			"keyword": ""
		}
	},
	"markdown": {
		"parser": "gfm",
		"hardwrap": true,
		"tags": ["examples"]
	},
    "opts": {
        "lenient": true,
        "encoding": "utf8"
    }
}
